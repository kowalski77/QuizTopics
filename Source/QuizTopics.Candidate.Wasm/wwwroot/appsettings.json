{
  "oidc": {
    "Authority": "https://localhost:5001/",
    "ClientId": "candidateclient",
    "DefaultScopes": [
      "openid",
      "profile",
      "email",
      "candidateapi"
    ],
    "RedirectUri": "https://localhost:5005/authentication/login-callback",
    "PostLogoutRedirectUri": "https://localhost:5005/authentication/logout-callback",
    "ResponseType": "code"
  },
  "UserOptions": {
    "NameClaim": "email"
  }
}